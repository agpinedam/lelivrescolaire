<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" configfailurepolicy="continue">
    <test name="Webkit" parallel="classes">
        <parameter name="browserName" value="WebKit"></parameter>
        <classes>
            <class name="test.ManuelGeographieSuite"/>
            <class name="test.Registration"/>
        </classes>
    </test>
    <test name="Chrome" parallel="classes">
        <parameter name="browserName" value="Chrome"></parameter>
        <classes>
            <class name="test.ManuelGeographieSuite"/>
            <class name="test.Registration"/>
        </classes>
    </test>
    <test name="Firefox" parallel="classes">
        <parameter name="browserName" value="Firefox"></parameter>
        <classes>
            <class name="test.ManuelGeographieSuite"/>
            <class name="test.Registration"/>
        </classes>
    </test>
</suite>